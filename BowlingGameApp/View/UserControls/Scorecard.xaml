<UserControl x:Class="BowlingGameApp.View.UserControls.Scorecard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BowlingGameApp.View.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="1000">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <local:FrameUserControl FrameNumber="1"/>
            <local:FrameUserControl FrameNumber="2"/>
            <local:FrameUserControl FrameNumber="3"/>
            <local:FrameUserControl FrameNumber="4"/>
            <local:FrameUserControl FrameNumber="5"/>
            <local:FrameUserControl FrameNumber="6"/>
            <local:FrameUserControl FrameNumber="7"/>
            <local:FrameUserControl FrameNumber="8"/>
            <local:FrameUserControl FrameNumber="9"/>
            <local:FrameUserControl FrameNumber="10"/>
        </StackPanel>
    </Grid>
</UserControl>
